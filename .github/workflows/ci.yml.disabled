on:
  push: { branches: [main, develop] }
  pull_request: { branches: [main] }
jobs:
  test:
    if: false  # disables this job
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-java@v4
        with:
          java-version: '21'
          distribution: 'temurin'
      - run: chmod +x gradlew
      - run: ./gradlew apiTests
      - run: ./gradlew uiTests